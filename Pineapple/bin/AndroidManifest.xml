<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pineapple"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppBaseTheme" >
        <activity
            android:name="com.example.pineapple.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.pineapple.GameActivity"
            android:label="@string/title_activity_game">
        </activity>
        <activity
            android:name="com.example.pineapple.BaseActivity"
            android:label="@string/title_activity_base" >
        </activity>
        <activity
            android:name="com.example.pineapple.LevelCompleteActivity"
            android:label="@string/title_activity_level_complete" >
        </activity>
        <activity
            android:name="com.example.pineapple.GameOverActivity"
            android:label="@string/title_activity_game_over" >
        </activity>
        	
    </application>

</manifest>